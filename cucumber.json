{
    "default": {
      "formatOptions": {
        "snippetInterface": "async-await"
      },
      "paths": ["src/test/features/"],
      "publishQuiet": true,
      "dryRun": false,
      "require": [
        "src/test/steps/*.ts",
        "src/test/Utils/*.ts"
      ],
      "requireModule": [
        "ts-node/register"
      ],

      "format": [
        "html:test-result/cucumber-report.html",
      "json:test-result/cucumber-report.json",
      "rerun:report/rerun.txt"
    ]
    }
  }

  